{
  "name": "@egy186/eslint-config",
  "description": "Eslint shareable config",
  "version": "3.1.0",
  "author": "egy186",
  "bugs": {
    "url": "https://github.com/egy186/eslint-config/issues"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "~4.4.1",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jsdoc": "~50.7.1",
    "eslint-plugin-n": "~17.19.0",
    "globals": "^16.2.0"
  },
  "devDependencies": {
    "@eslint/js": "~9.28.0",
    "@types/node": "^22.15.30",
    "@vitest/eslint-plugin": "~1.2.5",
    "eslint": "~9.28.0",
    "eslint-import-resolver-typescript": "~4.4.3",
    "eslint-plugin-jest": "~28.13.0",
    "eslint-plugin-react": "~7.37.5",
    "eslint-plugin-react-hooks": "~5.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.33.1"
  },
  "engines": {
    "node": ">=20.19.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./jest": {
      "types": "./dist/jest.d.ts",
      "default": "./dist/jest.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "default": "./dist/react.js"
    },
    "./typescript": {
      "types": "./dist/typescript.d.ts",
      "default": "./dist/typescript.js"
    },
    "./vitest": {
      "types": "./dist/vitest.d.ts",
      "default": "./dist/vitest.js"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/egy186/eslint-config#readme",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "peerDependencies": {
    "@vitest/eslint-plugin": "^1.2.5",
    "eslint": "^9.27.0",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-jest": "^28.13.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.32.0"
  },
  "peerDependenciesMeta": {
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "@eslint/compat": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egy186/eslint-config.git"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "npm run build -- --clean",
    "lint": "eslint",
    "prepare": "husky",
    "test": "jiti test/test.ts",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "type": "module",
  "types": "./dist/index.d.ts"
}
